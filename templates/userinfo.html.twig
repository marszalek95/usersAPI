{% extends 'base.html.twig' %}

{% block main %}
<div class="table-responsive col-md-6 container">
  
 <table class="table table-striped table-bordered">
 <tr>
 <th>First Name</th>
 <td>{{ user.first_name }}</td>
 </tr>
 <tr>
 <th>Last Name</th>
 <td>{{ user.last_name }}</td>
 </tr>
 <tr>
 <th>Gender</th>
 <td>{{ user.gender }}</td> </tr>
 <tr>
 <th>Born</th>
 <td>{{ user.dob }}</td> </tr>
 <tr>
 <th>Phone</th>
 <td>{{ user.phone }}</td> </tr>
 <tr>
 <th>Website</th>
 <td>{{ user.website }}</td> </tr>
 <tr>
 <th>Address</th>
 <td>{{ user.address }}</td> </tr>
 <tr>
 <th>Status</th>
 <td>{{ user.status }}</td> </tr>
 </table>
   
 </div>

{% endblock %} 
